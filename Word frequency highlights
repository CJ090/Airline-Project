wordfreq_pos = {}

words = pos_set

for word in pos_set[0]:
    if word not in wordfreq_pos:
        wordfreq_pos[word] = 0 
    wordfreq_pos[word] += 1
    
for w, f in wordfreq_pos.items():
    if f > 1:
        print(w,f)
